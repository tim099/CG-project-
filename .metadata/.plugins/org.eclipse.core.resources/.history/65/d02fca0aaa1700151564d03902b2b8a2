#ifndef STRING_H_
#define STRING_H_
#include <string>
namespace Tim {
class String {
public:
	String();
	virtual ~String();
	static bool within(char c,const std::string& str);
	static void gen_array_num(char *str,int num);
	static void skip_blank_space(char *str);
	static int skip(char *str,std::string skip);
};
}
#endif /* STRING_H_ */
