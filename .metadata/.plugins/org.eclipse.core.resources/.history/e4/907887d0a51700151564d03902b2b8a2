namespace Tim {
String::String() {

}
String::~String() {

}
void String::gen_array_num(char *str,int num){
	sprintf(str,"[%d]",num);
}
void String::skip(char *str,std::string skip){

}
void String::skip_blank_space(char *str){
	int i,start=-1;
	std::string cskip(" 	");
	for(i=0;str[i]!='\0';i++){
		if(start==-1){//not start yet
			for(unsigned j=0;j<cskip.length();j++){
				if(str[i]==cskip[j])break;
				if(j==cskip.length()-1)start=i;//start
			}
		}
		//if(start==-1&&str[i]!=' '&&str[i]!='\t'){//not start yet
			//start=i;
		//}
		if(start>0){
			str[i-start]=str[i];
		}
	}
	if(start!=-1)str[i-start]='\0';

}

}
