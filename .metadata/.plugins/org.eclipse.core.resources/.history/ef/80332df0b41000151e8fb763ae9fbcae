#ifndef TEXTURE2D_H_
#define TEXTURE2D_H_
#include "class/texture/texture.h"
class Texture2D : public Texture {
public:
	Texture2D(GLuint TexID,int width,int height);
	virtual ~Texture2D();
	virtual void use_texture(GLuint programID,int num,const char *name);
	int width,height;
protected:

};

#endif /* TEXTURE2D_H_ */
